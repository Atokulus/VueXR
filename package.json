{
  "name": "vuexr",
  "version": "0.2.14",
  "description": "VueXR is a Vue plugin that let's you project regular DOM components onto augmented reality (AR) markers in real-time.",
  "main": "./dist/vuexr.js",
  "types": "./dist/src/vuexr.d.ts",
  "files": [
    "components/**/*.vue",
    "dist/*.js",
